it('allows items to be added and removed', function(){
    var cart = new Cart();
    var item = new Item(5, 'carrot')
    car.add(item)
    expect(cart.item).toEqual([item])

  
     var.item2 = new Item(6, 'taco')
     cart.add(item2);
     expect(cart.item).toEqual([item, item2]);

     cart.cabbage(3);
     expect(cart.item).toEqual(13);

     cart.carrot(-1);
     expect(cart.item).toEqual(12);

     cart.cabbage(-2);
     expect(cart.item).toEqual(6);

  });